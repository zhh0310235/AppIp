/* pages/main/Cjuan/rate/touchBiao/classTable.wxss */
.xinxi{
  width: 100%;
  height: 40px;
  line-height: 40px;
  background-color: #eee;

}
.xinxi view:nth-child(1){
  font-family: 微软雅黑;
  color: #555;
  font-size: 14px;
  font-weight: 700;
  float: left;
  padding-left:10px; 
}
.xinxi view:nth-child(2){
  font-family: 微软雅黑;
  color: #555;
  font-size: 14px;
  font-weight: 700;
  float: right;
  padding-right:10px; 
}
.xinxi view:nth-child(3){
  font-family: 微软雅黑;
  width: 100%;
  height: 25px;
  line-height: 25px;
  text-align: center;
  color: #555;
  font-size: 13px;
  font-weight: 700;
  clear: both;
}
.timeX{
  width: 100%;
  height: 30px;
  line-height: 30px;
  font-family: 微软雅黑;
  font-size: 12px;
  color: #555;
  clear: both;
  margin-top: 25px;
}
.timeX view{
  width: 18%;
  text-align: center;
  font-size: 12px;
  box-sizing: border-box;
  border: 1px dashed #ccc;
  background-color: #eee;
  border-right: 0;
  float: left;
}
.timeX view:nth-child(6){
  border-right:1px dashed #ccc;
}
.timeX view:nth-child(1){
  border:0;
  width: 10%;
  background-color: #FF6600;
  color: #fff;
  font-weight: 700;
}
.timeY{
  width: 10%;
  font-family: 微软雅黑;
  color: #555;
  position: relative;
  top: -35px;
  font-size: 13px;
  clear: both;
  margin-top: 35px;
}
.timeY view{
  width: 100%;
  height: 120px;
  line-height: 120px;
  text-align: center;
  font-size: 13px;
  box-sizing: border-box;
  border: 1px dashed #ccc;
  background-color: #eee;
  border-top: 0;
}
.timeY view:nth-child(1){
  border-top:1px dashed #ccc;
}
.classContent{
  position: absolute;
  top:95px;
  left: 10%;
  width: 90%;
}
.classContent>view{
  width: 18%;
  margin-left: 1%;
  margin-right: 1%;
  box-sizing: border-box;
  font-family: 微软雅黑;
  font-size: 12px;
  float: left;
  padding-top:2px; 
}
.classContent>view>view{
  width: 100%;
  height: 120px;
  box-sizing: border-box;
  border: 1px solid #ccc;
  border-top: 0;
}