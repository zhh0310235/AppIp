ul li{
    width: 100%;
    height: 70px;
    border-bottom:1px solid #ebebeb ;
    padding: 5px 10px;
}

ul li view:nth-of-type(1){
    width: 55px;
    height: 55px;
    background-size: contain;/*背景图片充满*/
    border-radius: 20px;       /* 图片圆角属性*/
    float: left;
    margin-left: 50rpx;
}
